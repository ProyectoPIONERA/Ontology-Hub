  <!-- Ontology Information -->
      
  <owl:Ontology rdf:about="http://purl.org/vocommons/voaf">
       <dc:title xml:lang="en">Vocabulary of a Friend</dc:title>
       <dc:title xml:lang="fr">Vocabulaire d'un ami</dc:title>
       <rdf:type rdf:resource="http://purl.org/vocommons/voaf#Vocabulary"/>
       <dc:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2011-03-11</dc:issued>
       <dc:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2012-07-03</dc:modified>
       <dc:description xml:lang="en">A vocabulary to describe linked data vocabularies and their relations.</dc:description>
       <dc:description xml:lang="fr">Un vocabulaire de description des vocabulaires RDF et de leurs relations mutuelles</dc:description>
       <dc:contributor>
              <foaf:Person rdf:about="http://data.semanticweb.org/person/lise-rozat"/>
       </dc:contributor>
       <dc:contributor>
              <foaf:Person rdf:about="http://data.semanticweb.org/person/pierre-yves-vandenbussche"/>
       </dc:contributor>
