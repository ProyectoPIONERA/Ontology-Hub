                    <dc:date>2011-11-16</dc:date>
                    <rdfs:comment xml:lang="en">Depreciation of voaf:exampleDataset, replaced by voaf:dataset</rdfs:comment>
                    <rdfs:comment xml:lang="fr">Dépréciation de voaf:exampleDataset, remplacé par voaf:dataset</rdfs:comment>
                    <frbr:embodiment>
                          <frbr:Manifestation rdf:about="http://purl.org/vocommons/voaf/v1.1/voaf_v1.1.rdf">
                                <frbr:alternate>
                                      <frbr:Manifestation rdf:about="http://purl.org/vocommons/voaf/v1.1/index.html"/>
